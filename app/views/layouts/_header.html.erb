  <!--// LOGO //-->
  <div>
    <a href="/" id="completeset"></a>
  </div>
  <!--// RIGHT //-->
  <div class="right">
    
    <% if(current_user) %>
    
      <div id="activity_menu_button_container">
        <button type="button" id="activity_menu_button" name="activity_menu_button" class="ss-icon">0</button>
      </div>
      <div id="user_menu_button_container">
        <button type="button" id="user_menu_button" name="user_menu_button" class="ss-icon">settings</button>
      </div>
      <!--// USER MENU //-->
    <div id="user_menu_container" style="">
        <span class="caret top"></span>
        <div id="user_menu" class="dropbox">
          <ul class="user_menu_column">
            <!--<li><a href="{user-id}/shelves">Your Shelves</a></li>
