    <div class="hamburger">
      <button type="button" id="hamburger_menu_button" name="hamburger" class="ss-icon">rows</button>
    </div>
    <div class="search">
      <form id="search_form">
        <input id="search" type="text" autocomplete="off" placeholder="Search" />
        <button type="submit" class="ss-icon">search</button>
      </form>
    </div>
    <!--// HAMBURGER MENU //-->
    <div id="hamburger_menu_container" style="display: none;">
      <span class="caret top"></span>
      <div id="hamburger_menu" class="dropbox">
        <ul class="hamburger_menu_column_one">
