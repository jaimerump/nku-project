          <li><a href="/{brand}">Gallery 1988</a></li>
          <li><a href="/{brand}">Gum Cards</a></li>
        </ul>
        <ul class="hamburger_menu_column_three">
          <li><a href="/{brand}">Harry Potter</a></li>
          <li><a href="/{brand}">Hess</a></li>
          <li><a href="/{brand}">Indiana Jones</a></li>
          <li><a href="/{brand}">HOMAGE</a></li>
          <li><a href="/{brand}">James Bond 007</a></li>
