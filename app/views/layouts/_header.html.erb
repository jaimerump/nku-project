            <li><a href="/curate">Curate</a></li>-->
            <li><a href="/logout">Log Out</a></li>
          </ul>
        </div>
      </div>
    
    <% else %>
    
      <div id="access_menu_container">
        <button type="button" name="join">Join</button>
        <button type="button" name="login">Login</button>
      </div>
    
    <% end %>
    
  </div>
</header>

<%= javascript_include_tag "header" %>